{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "removeComments": true,//移除注释
    "noEmitOnError": true,//源码未变已通过，则不生成结果
    "lib":["esnext"] //编译环境
  },
  "include": ["./src"]
}
